fx(a1,z1,a2,z2)=27.1*(a1**0.333+a2**0.3333)**2/z1/z2
frb(a1,z1,a2,z2)=1.3*(a1**0.3333+a2**0.3333)+0.65*log(fx(a1,z1,a2,z2))
fvb(a1,z1,a2,z2)=z1*z2*1.44/frb(a1,z1,a2,z2)-15/(fx(a1,z1,a2,z2)+1)

print fvb(12,6,12,6),  fvb(12,6,12,6)-fvb(12,6,13,6),  fvb(12,6,13,6)-fvb(13,6,13,6)

